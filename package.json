{
  "name": "moonlight",
  "private": true,
  "version": "1.3.39",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "description": "Yet another Discord mod",
  "license": "LGPL-3.0-or-later",
  "homepage": "https://moonlight-mod.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonlight-mod/moonlight.git"
  },
  "bugs": {
    "url": "https://github.com/moonlight-mod/moonlight/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=22",
    "pnpm": ">=10",
    "npm": "pnpm",
    "yarn": "pnpm"
  },
  "scripts": {
    "build": "pnpm run --filter=!browser -r build",
    "browser": "pnpm run --filter=browser build",
    "dev": "pnpm run --parallel --stream dev",
    "clean": "pnpm run --filter=!esbuild-config -r clean",
    "lint": "eslint --ignore-pattern \"**/dist/**\"",
    "lint:fix": "pnpm lint --fix",
    "lint:report": "pnpm lint --output-file eslint_report.json --format json",
    "typecheck": "pnpm run -r typecheck",
    "check": "pnpm run lint && pnpm run typecheck",
    "prepare": "husky install",
    "updates": "pnpm taze -r"
  },
  "devDependencies": {
    "@moonlight-mod/eslint-config": "catalog:dev",
    "eslint": "catalog:dev",
    "husky": "catalog:dev",
    "prettier": "catalog:dev",
    "taze": "catalog:dev"
  }
}
